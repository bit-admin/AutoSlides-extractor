{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "AutoSlidesExtractor",
  "version": "0.1.0",
  "identifier": "com.tauri.dev",
  "build": {
    "frontendDist": "../src/renderer",
    "devUrl": "../src/renderer"
  },
  "app": {
    "windows": [
      {
        "title": "AutoSlides Extractor",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all"
  }
}
